{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "arrow-return-shorthand": true,
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "interface-name": false,
    "member-access": true,
    "member-ordering": [
      true,
      {
        "order": [
          "static-field",
          "instance-field",
          "static-method",
          "instance-method"
        ]
      }
    ],
    "quotemark": [true, "single", "avoid-escape", "avoid-template"],
    "trailing-comma": false
  },
  "rulesDirectory": []
}
