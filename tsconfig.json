{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "dom",
      "es7"
    ],
    "types": [
      "chrome"
    ],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "strictNullChecks": false,
    "sourceMap": true,
    "outDir": "./dist",
    "strict": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true
  }
}
